<!doctype html>
<html lang="en" ng-app="wellshanaApp">
<head>
  <meta charset="utf-8">
  <title>WellShana Home</title>
  <link rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap.css" type"text/css">
  <!--<link rel="stylesheet" href="stylesheets/bootstrap-new.css">-->
  <link rel="stylesheet" href="stylesheets/fullcalendar.css" type="text/css"/>
  <link rel="stylesheet" href="/bower_components/components-font-awesome/css/font-awesome.min.css">
  <link rel="stylesheet" href="/bower_components/ng-password-meter/dist/ng-password-meter.css">
  <link rel="stylesheet" href="/bower_components/bootstrap-switch/dist/css/bootstrap3/bootstrap-switch.css">
  <link rel="stylesheet" href="stylesheets/mystyle.css" type="text/css"/>
  <link rel="stylesheet" href="stylesheets/creditcard.css" type="text/css"/>
  <!-- for CSS Transitions and/or Keyframe Animations -->
  <!--<link rel="stylesheet" href="css/animations.css">-->

  <!-- Load Javascript -->
  <script src="/bower_components/jquery/dist/jquery.js"></script>
  <script src="/bower_components/bootstrap/dist/js/bootstrap.js"></script>
  <script src="/bower_components/angular/angular.js"></script>
  <script src="/bower_components/angular-animate/angular-animate.js"></script>
  <script src="/bower_components/angular-ui-router/release/angular-ui-router.js"></script>
  <script src="/bower_components/angular-bootstrap/ui-bootstrap-tpls.js"></script>
  <script src="/bower_components/angular-sanitize/angular-sanitize.js"></script>
  <script src="/bower_components/angular-resource/angular-resource.js"></script>
  <script src="/bower_components/angular-random-string/src/angular-random-string.js"></script>
  <script src="/bower_components/bootstrap-switch/dist/js/bootstrap-switch.js"></script>
  <script src="/bower_components/angular-bootstrap-switch/dist/angular-bootstrap-switch.js"></script>
  <script src="/bower_components/ng-idle/angular-idle.js"></script>
  <script src="/bower_components/a0-angular-storage/dist/angular-storage.js"></script>
  <script src="/bower_components/webrtc-adapter/release/adapter.js"></script>
  <script type="text/javascript" src="/bower_components/moment/min/moment.min.js"></script>
  <script type="text/javascript" src="/bower_components/later/later.js"></script>
  <script type="text/javascript" src="/bower_components/angular-ui-calendar/src/calendar.js"></script>
  <script type="text/javascript" src="/bower_components/fullcalendar/dist/fullcalendar.js"></script>
  <script type="text/javascript" src="/bower_components/fullcalendar/dist/gcal.js"></script>
  <script src="/bower_components/ng-password-meter/dist/ng-password-meter.js"></script>
  <script src='javascripts/moment.min.js'></script>
  <script src="javascripts/angular/angularApp.js"></script>
  <script src="javascripts/angular/animations.js"></script>
  <script src="javascripts/angular/controllers.js"></script>
  <script src="javascripts/angular/directives.js"></script>
  <script src="javascripts/angular/filters.js"></script>
  <script src="javascripts/angular/services.js"></script>
  <script src="javascripts/simplewebrtc.bundle.js" type="text/javascript"></script>

  <title>Wellshana</title>
    </head>
    <body>
      <nav class="navbar navbar-fixed-top navbar-inverse">
        <div class="container-fluid">
          <div class="row">
            <div class="col-md-4">
              <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                  <span class="sr-only">Toggle navigation</span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                </button>
                <div><a class="navbar-brand" ui-sref="main.home">Wellshana&trade;</a></div>
              </div>
            </div>
          </div> <!--end of row -->
          <div class="row">
            <div class="col-md-12">
              <div class="mission">Wellness starts here. &trade;</div>
            </div>
          </div>
          <div class="row">
            <div class="col-md-12">
              <div id="navbar" class="collapse navbar-collapse" ng-controller="HeaderCtrl">
                <ul class="nav navbar-nav">
                  <li ng-class="{active: isActive('/profile')}"><a ui-sref="profile">My Profile</a></li>
                  <!--<li ng-class="{active: isActive('/appointments')}"><a ui-sref="appt.search">Appointments</a></li>-->
                  <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Appointments <span class="caret"></span></a>
                    <ul class="dropdown-menu">
                      <li ng-class="{active: isActive('/appointments')}"><a ui-sref="appt.search">Book an Appointment</a></li>
                      <li ng-class="{active: isActive('/appointments')}"><a ui-sref="appt.upcoming">Upcoming Appointments</a></li>
                    </ul>
                  </li>
                  <li ng-class="{active: isActive('/inbox')}"><a ui-sref="inbox" ui-sref-opts="{reload: true}">Inbox</a></li>
                  <li ng-class="{active: isActive('/notes')}"><a ui-sref="notes">Notes</a></li>
                </ul>
                <div ng-show="isLoggedIn===true" class="btn-group pull-right">
                  <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                    <span class="glyphicon glyphicon-user" aria-hidden="true"></span> {{ currentUser.name.firstName }}
                    <span class="caret"></span>
                  </button>
                  <ul class="dropdown-menu" role="menu">
                    <li><a ui-sref="profile">Profile</a></li>
                    <li><a ui-sref="account.payment">Account</a></li>
                    <li><a ui-sref="appt.upcoming">Appointments</a></li>
                    <li><a ui-sref="messages">Messages</a></li>
                    <li class="divider"></li>
                    <li><a ui-sref="main.home" ng-click="logout()">Logout</a></li>
                  </ul>
                </div>
                <button ng-hide="isLoggedIn===true" class="btn pull-right" ui-sref="login">Login</button>
                <button ng-hide="isLoggedIn===true" class="btn btn-primary pull-right" ui-sref="signup.start">Signup</button>
              </div><!-- /.nav-collapse -->
            </div>
          </div><!-- row-->
        </div><!-- /.container -->
      </nav><!-- /.navbar -->
      <div class="view-container">
        <ui-view></ui-view>
      </div>
      <footer class="footer" style="text-align: center;"><p>All rights reserved.</p><a ng-hide="isLoggedIn===true" ui-sref="signup.start({therapistRegistration: true })">Signup For Therapists</a></footer>
    </body>
</html>
